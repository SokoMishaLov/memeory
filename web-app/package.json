{
  "name": "memeory",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "antd": "^3.23.6",
    "antd-mobile": "^2.3.1",
    "axios": "^0.19.0",
    "copy-to-clipboard": "^3.2.0",
    "firebase": "~7.2.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.10.2",
    "react-aspect-ratio": "~1.0.41",
    "react-device-detect": "^1.9.10",
    "react-dom": "^16.10.2",
    "react-hooks-async": "^3.4.2",
    "react-infinite-scroller": "~1.2.4",
    "react-player": "^1.13.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-with-firebase-auth": "~1.1.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "babel-plugin-import": "~1.12.2",
    "customize-cra": "~0.8.0",
    "lessc": "~1.0.2",
    "node-sass-chokidar": "1.3.5",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "~2.1.3",
    "react-dev-utils": "~9.1.0",
    "react-scripts": "~3.2.0",
    "source-map-explorer": "~2.1.0",
    "webpack-ant-icon-loader": "~1.0.8"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "build-less": "lessc --js src/app/common/styles/antd.less src/app/common/styles/antd.css",
    "watch-css": "npm run build-less && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "npm run build-less && node-sass-chokidar src/ -o src/",
    "build": "npm run build-css && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
