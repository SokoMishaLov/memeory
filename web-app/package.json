{
  "name": "memeory",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sokomishalov.github.io/memeory",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~1.2.25",
    "@fortawesome/free-brands-svg-icons": "~5.12.0",
    "@fortawesome/free-solid-svg-icons": "~5.11.2",
    "@fortawesome/react-fontawesome": "~0.1.7",
    "antd": "^3.25.3",
    "antd-mobile": "^2.3.1",
    "axios": "^0.19.0",
    "copy-to-clipboard": "~3.2.0",
    "detect-browser-language": "0.0.2",
    "firebase": "~7.5.0",
    "i18next": "~19.0.1",
    "i18next-browser-languagedetector": "~4.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-aspect-ratio": "~1.0.41",
    "react-device-detect": "^1.9.10",
    "react-dom": "^16.12.0",
    "react-i18next": "~11.2.2",
    "react-infinite-scroller": "~1.2.4",
    "react-player": "^1.14.2",
    "react-reveal": "^1.2.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-with-firebase-auth": "~1.2.0"
  },
  "devDependencies": {
    "babel-plugin-import": "~1.13.0",
    "customize-cra": "~0.9.1",
    "gh-pages": "^2.1.1",
    "less": "^3.10.3",
    "node-sass-chokidar": "1.4.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "~15.7.2",
    "react-app-rewired": "~2.1.5",
    "react-scripts": "~3.2.0",
    "source-map-explorer": "~2.1.1",
    "webpack-ant-icon-loader": "~1.0.8"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "react-app-rewired start",
    "build-less": "lessc --js src/app/common/styles/antd.less src/app/common/styles/antd.css",
    "watch-css": "npm run build-less && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css": "npm run build-less && node-sass-chokidar src/ -o src/",
    "build": "npm run build-css && react-app-rewired build",
    "predeploy": "npm run build && cp build/index.html build/404.html",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
