{
  "name": "web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jetbrains/kotlin-css": "~1.0.0-pre.83",
    "@jetbrains/kotlin-css-js": "~1.0.0-pre.83",
    "@jetbrains/kotlin-react-redux": "~5.0.7-pre.83",
    "@jetbrains/kotlin-styled": "~1.0.0-pre.83",
    "@kotlinspain/kotlin-antd": "~3.20.3-pre.11",
    "@kotlinspain/kotlin-moment": "~2.24.0-pre.6",
    "antd": "~3.23.5",
    "inline-style-prefixer": "~5.1.0",
    "kotlinx-coroutines-core": "~1.3.2",
    "moment": "~2.24.0",
    "react": "^16.10.2",
    "react-device-detect": "^1.9.10",
    "react-dom": "^16.10.2",
    "styled-components": "~4.4.0"
  },
  "devDependencies": {
    "node-sass-chokidar": "1.3.5",
    "react-scripts-kotlin": "4.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-kotlin start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts-kotlin build",
    "eject": "react-scripts-kotlin eject",
    "gen-idea-libs": "react-scripts-kotlin gen-idea-libs",
    "get-types": "react-scripts-kotlin get-types --dest=src/types",
    "postinstall": "npm run gen-idea-libs"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
